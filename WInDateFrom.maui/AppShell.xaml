<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WinDateFrom.maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WinDateFrom.maui"
    Shell.FlyoutBehavior="Flyout">
    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems" Shell.TabBarIsVisible="False">
        <ShellContent x:Name="scMain" Title="" ContentTemplate="{DataTemplate local:MainPage}" Route="Main"/>
        <ShellContent x:Name="scInfo" ContentTemplate="{DataTemplate local:InfoPage}" Route="Informazioni"/>
    </FlyoutItem>

</Shell>
